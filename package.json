{
  "name": "bookmarks-concise",
  "displayName": "bookmarks-concise",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
   "files": [
    "out",
    "assets"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "bookmark-view-container",
          "title": "Bookmarks",
          "icon": "assets/bookmark.svg"
        }
      ]
    },
    "views": {
      "bookmark-view-container": [
        {
          "icon": "./src/assets/bookmark.svg",
          "id": "bookmarkView",
          "name": "Bookmarks"
        }
      ]
    },
    "commands": [
      {
        "command": "bookmark.open",
        "title": "Open Bookmark"
      },
      {
        "command": "bookmark.add",
        "title": "Add Bookmark",
        "shortTitle": "+"
      },
      {
        "command": "bookmark.delete",
        "title": "Delete Bookmark",
        "shortTitle": "X"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "bookmark.delete",
          "when": "view == bookmarkView"
        }
      ],
      "view/title": [
        {
          "command": "bookmark.add",
          "when": "view == bookmarkView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "icon": "assets/icon.png",
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  },
  "publisher": "sola-xx",
  "repository": {
    "type": "git",
    "url": "https://github.com/xuxiao1797/vscode-bookmarks-concise.git"
  }
}
